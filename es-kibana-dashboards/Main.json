{
    "title": "Main",
    "services": {
        "query": {
            "list": {
                "0": {
                    "query": "*",
                    "alias": "",
                    "color": "#7EB26D",
                    "id": 0,
                    "pin": false,
                    "type": "lucene",
                    "enable": true,
                }
            },
            "ids": [0],
        },
        "filter": {
            "list": {
                "0": {
                    "type": "terms",
                    "field": "_type",
                    "value": "audit",
                    "mandate": "mustNot",
                    "active": true,
                    "alias": "",
                    "id": 0,
                },
                "1": {
                    "type": "terms",
                    "field": "_type",
                    "value": "ac",
                    "mandate": "mustNot",
                    "active": true,
                    "alias": "",
                    "id": 1,
                },
                "2": {
                    "type": "terms",
                    "field": "_type",
                    "value": "timeouts",
                    "mandate": "mustNot",
                    "active": true,
                    "alias": "",
                    "id": 2,
                },
            },
            "ids": [0, 1, 2],
        },
    },
    "rows": [
        {
            "title": "Graph",
            "height": "550px",
            "editable": true,
            "collapse": false,
            "collapsable": true,
            "panels": [
                {
                    "error": false,
                    "span": 7,
                    "editable": true,
                    "group": ["default"],
                    "type": "terms",
                    "queries": {"mode": "selected", "ids": [0]},
                    "field": "_type",
                    "exclude": ["tests", "temp", "dashboard"],
                    "missing": false,
                    "other": false,
                    "size": 10,
                    "order": "count",
                    "style": {"font-size": "10pt"},
                    "donut": false,
                    "tilt": false,
                    "labels": true,
                    "arrangement": "horizontal",
                    "chart": "table",
                    "counter_pos": "above",
                    "spyable": true,
                    "title": "Document Types     (These are the table names with number of records of each type of data , for example , the jenkins monitoring data is stored in table 'builds-data')",
                    "tmode": "terms",
                    "tstat": "total",
                    "valuefield": "",
                },
                {
                    "error": false,
                    "span": 4,
                    "editable": true,
                    "type": "text",
                    "loadingEditor": false,
                    "mode": "markdown",
                    "content": "The table at the left shows a summary of  types of data that is currently stored in elastic-search i.e these are the table names with number of records/rows , in order to see the details and separate dashboards , please click on the folder/load icon on top right corner \nthat contains a drop down list of links to these tables. There is a query behind each dashboard or graph , that can be viewed by going the star/settings icon for each graph. To view all the queries , click on the query tab at top left for each dashboard.",
                    "style": {},
                    "title": "help",
                },
            ],
            "notice": false,
        }
    ],
    "editable": true,
    "index": {
        "interval": "none",
        "pattern": "[logstash-]YYYY.MM.DD",
        "default": "*",
        "warm_fields": false,
    },
    "style": "dark",
    "failover": false,
    "panel_hints": true,
    "loader": {
        "save_gist": false,
        "save_elasticsearch": true,
        "save_local": true,
        "save_default": true,
        "save_temp": true,
        "save_temp_ttl_enable": true,
        "save_temp_ttl": "30d",
        "load_gist": true,
        "load_elasticsearch": true,
        "load_elasticsearch_size": 20,
        "load_local": true,
        "hide": false,
    },
    "pulldowns": [
        {
            "type": "query",
            "collapse": true,
            "notice": false,
            "query": "*",
            "pinned": true,
            "history": [
                "*",
                "_index:ib-matrix*",
                "_index:*",
                "_index:jenkins",
                "_index:ib-matrix-*",
                "_type:builds-data",
                "_type:unittests",
            ],
            "remember": 10,
            "enable": true,
        },
        {"type": "filtering", "collapse": true, "notice": false, "enable": true},
    ],
    "nav": [
        {
            "type": "timepicker",
            "collapse": false,
            "notice": false,
            "status": "Stable",
            "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"],
            "refresh_intervals": [
                "5s",
                "10s",
                "30s",
                "1m",
                "5m",
                "15m",
                "30m",
                "1h",
                "2h",
                "1d",
            ],
            "timefield": "@timestamp",
            "enable": true,
            "now": true,
            "filter_id": 2,
        }
    ],
    "refresh": false,
}
